<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:viewModel="clr-namespace:Cashbox.MVVM.ViewModels"
                    xmlns:view="clr-namespace:Cashbox.MVVM.Views.Pages">

    <DataTemplate DataType="{x:Type TypeName=viewModel:AuthViewModel}">
        <view:AuthView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type TypeName=viewModel:LoadAppViewModel}">
        <view:LoadScreen/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type TypeName=viewModel:LoadingViewModel}">
        <view:LoadingView/>
    </DataTemplate>
</ResourceDictionary>